{
  "env": {
    "commonjs": true,
    "node": true,
    "browser": true
  },
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "react/forbid-prop-types": 2,
    "react/no-multi-comp": [2, { "ignoreStateless": false }],
    "react/no-set-state": 2,
    "react/no-string-refs": 0,
    "react/prefer-es6-class": [2, "never"],
    "react/self-closing-comp": 2,
    "react/sort-prop-types": [2, {
      "callbacksLast": true,
      "ignoreCase": false,
      "requiredFirst": true,
    }],
    "react/wrap-multilines": [2, {
      "return": true
    }],
    "react/require-extension": [2, { "extensions": [".js", ".jsx"] }],
    "react/jsx-curly-spacing": [2, "always"],
    "react/prop-types": [2, {"ignore": ["params", "children"]}],
    "react/display-name": 0

  },
  "globals": {
    "fetch": true
  }
}